#include <stdio.h>

int main() {
    int num1, num2, num3;

    // Solicita os três valores do usuário
    printf("Digite o primeiro valor: ");
    scanf("%d", &num1);
    printf("Digite o segundo valor: ");
    scanf("%d", &num2);
    printf("Digite o terceiro valor: ");
    scanf("%d", &num3);

    // Verifica qual é o maior valor
    int maior = num1; // Suponha que o primeiro número é o maior inicialmente
    if (num2 > maior) {
        maior = num2;
    }
    if (num3 > maior) {
        maior = num3;
    }

    // Mostra o maior valor
    printf("O maior valor digitado é: %d\n", maior);

    return 0;
}
